function [lengths, twoValImg] = getLengths(image)

[height, width] = size(image);
twoValImg = twoValImage(image);
count = 0;

for i = 1 : width
    for j = 1 : height
        if twoValImg(j, i) == 0
           startRowNums(count) = j;
           startColNums(count) = i;
           count = count + 1;
           twoValImg = bfs(twoValImg, height, width, i, j);           
        end
    end
end

lengths = count;
